{
  "expo": {
    "name": "TeleMedApp",
    "slug": "TeleMedApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.anonymous.telemedapp",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow TeleMedApp to access your camera for video calls",
          "microphonePermission": "Allow TeleMedApp to access your microphone for video calls",
          "recordAudioAndroid": true
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "52f545d8-9077-4b56-9af4-447ea29efbc3"
      }
    },
    "owner": "chawit"
  }
}
